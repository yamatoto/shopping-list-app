{
  "cli": {
    "version": ">= 10.2.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote",
        "image": "latest",
        "env": {
          "GOOGLE_SERVICE_INFO_PLIST": "{{GOOGLE_SERVICES_JSON}}"
        }
      },
      "ios": {
        "credentialsSource": "remote",
        "image": "latest",
        "env": {
          "GOOGLE_SERVICE_INFO_PLIST": "{{GOOGLE_SERVICE_INFO_PLIST}}"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
